.detail-container {
  padding: 30rpx;
  background-color: #f5f5f5;
  min-height: 100vh;
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}

.action-buttons {
  display: flex;
}

.content-card {
  background-color: white;
  border-radius: 16rpx;
  padding: 30rpx;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.05);
  margin-bottom: 30rpx;
}

.task-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.cell-description {
  white-space: normal;
  word-break: break-all;
}

.priority-display {
  display: flex;
  align-items: center;
}

.priority-dot {
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.priority-high {
  background-color: #ff4d4f;
}

.priority-medium {
  background-color: #faad14;
}

.priority-low {
  background-color: #52c41a;
}

.loading-center {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 200rpx;
}

.timer-section {
  background-color: white;
  border-radius: 16rpx;
  margin-bottom: 30rpx;
  overflow: hidden;
}

.timer-info {
  font-size: 28rpx;
  color: #666;
}

.footer-actions {
  display: flex;
  flex-direction: column;
  gap: 20rpx;
}

.delete-btn {
  margin-top: 20rpx;
}

/* 子计划样式 */
.subtasks-section {
  background-color: white;
  border-radius: 16rpx;
  margin-bottom: 30rpx;
  padding: 30rpx;
}

.section-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}

.section-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  display: flex;
  align-items: center;
}

.subtask-count {
  font-size: 24rpx;
  color: #666;
  margin-left: 10rpx;
  font-weight: normal;
}

.add-subtask-btn {
  padding: 10rpx;
}

.subtasks-list {
  margin-top: 20rpx;
}

/* 子计划项样式 */
.subtask-item {
  margin-bottom: 16rpx;
  background-color: #f9f9f9;
  border-radius: 12rpx;
  padding: 16rpx;
  transition: all 0.3s ease;
}

.subtask-item:last-child {
  border-bottom: none;
}

.subtask-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}

.subtask-content {
  display: flex;
  align-items: flex-start;
  flex: 1;
}

.subtask-checkbox {
  margin-right: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.subtask-info {
  flex: 1;
}

.subtask-title {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 6rpx;
}

.subtask-item.completed .subtask-title {
  color: #999;
  text-decoration: line-through;
}

.subtask-meta {
  font-size: 24rpx;
  color: #999;
}

.subtask-progress {
  display: flex;
  align-items: center;
  margin-top: 8rpx;
  font-size: 24rpx;
  color: #999;
}

.subtask-children-count {
  background-color: #f0f0f0;
  padding: 4rpx 10rpx;
  border-radius: 10rpx;
  margin-right: 10rpx;
}

.subtask-actions {
  display: flex;
  align-items: center;
  gap: 16rpx;
}

.subtask-action {
  padding: 8rpx;
}

.empty-subtasks {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 40rpx 0;
}

.empty-text {
  font-size: 28rpx;
  color: #999;
  margin-bottom: 20rpx;
}

.add-btn {
  margin-top: 20rpx;
}

/* 计时器弹窗样式 */
.timer-modal,
.subtask-modal,
.subtask-detail-modal {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 1000;
  display: flex;
  justify-content: center;
  align-items: center;
  opacity: 0;
  pointer-events: none;
  transition: opacity 0.3s;
}

.timer-modal.show,
.subtask-modal.show,
.subtask-detail-modal.show {
  opacity: 1;
  pointer-events: auto;
}

.timer-modal-content,
.subtask-modal-content {
  width: 80%;
  background-color: white;
  border-radius: 24rpx;
  padding: 40rpx;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.1);
}

.subtask-modal-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}

.subtask-modal-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}

.subtask-form {
  margin-bottom: 30rpx;
}

.error-message {
  display: flex;
  align-items: center;
  background-color: #fff2f0;
  padding: 16rpx;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
  color: #e34d59;
  font-size: 26rpx;
}

.error-message text {
  margin-left: 10rpx;
}

.form-item {
  margin-bottom: 24rpx;
}

.form-label {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 12rpx;
}

.input-container {
  background-color: #f5f5f5;
  border-radius: 8rpx;
  padding: 16rpx;
}

.input-field {
  width: 100%;
  font-size: 28rpx;
  color: #333;
}

.subtask-modal-footer {
  margin-top: 30rpx;
}

.submit-btn {
  margin-top: 20rpx;
}

.subtask-detail-content {
  margin-bottom: 30rpx;
}

.nested-subtasks-section {
  margin-top: 30rpx;
  border-top: 1px solid #f0f0f0;
  padding-top: 20rpx;
}

.subtask-detail-actions {
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}

.action-btn {
  flex: 1;
  margin: 0 10rpx;
}

.timer-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 40rpx;
}

.timer-tabs {
  display: flex;
  background-color: #f5f5f5;
  border-radius: 8rpx;
  overflow: hidden;
}

.timer-tab {
  padding: 16rpx 30rpx;
  font-size: 28rpx;
  color: #666;
}

.timer-tab.active {
  background-color: #0052d9;
  color: white;
}

.close-icon {
  padding: 10rpx;
}

.timer-display {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 60rpx 0;
}

.timer-circle {
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
  border: 8rpx solid #e0e0e0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  overflow: hidden;
  transition: all 0.3s ease;
}

.timer-circle.running {
  border-color: #0052d9;
  box-shadow: 0 0 20rpx rgba(0, 82, 217, 0.3);
}

.timer-progress {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background: conic-gradient(#0052d9 0%, transparent 0%);
  transition: background 0.3s linear;
}

.timer-time {
  font-size: 60rpx;
  font-weight: bold;
  color: #333;
  z-index: 2;
}

.timer-label {
  font-size: 28rpx;
  color: #666;
  margin-top: 10rpx;
  z-index: 2;
}

.timer-tip {
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
  z-index: 2;
}

.timer-controls {
  display: flex;
  flex-direction: column;
  gap: 20rpx;
}

.timer-btn {
  margin-bottom: 20rpx;
}

.return-btn {
  margin-top: 10rpx;
}

.action-menu-popup {
  position: absolute;
  top: 100rpx;
  right: 20rpx;
  width: 200rpx;
  background: #fff;
  border-radius: 12rpx;
  box-shadow: 0 4rpx 16rpx rgba(0,0,0,0.12);
  z-index: 999;
}

.action-menu-popup .menu-item {
  padding: 24rpx 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #f5f5f5;
}

.action-menu-popup .menu-item:last-child {
  border-bottom: none;
}

.action-menu-popup .menu-item.delete {
  color: #e34d59;
}

.action-menu-popup .menu-icon {
  font-size: 28rpx;
  margin-left: 16rpx;
}

/* 子计划树状结构样式 */
.nested-subtasks {
  margin-left: 40rpx;
  border-left: 2rpx solid #e0e0e0;
  padding-left: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.tree-view {
  position: relative;
}

.subtask-item.nested {
  position: relative;
  padding-left: 10rpx;
  margin-bottom: 10rpx;
  background-color: transparent;
  border-radius: 0;
  padding: 10rpx 0;
}

.subtask-item.nested:before {
  content: "";
  position: absolute;
  left: -20rpx;
  top: 20rpx;
  width: 20rpx;
  height: 2rpx;
  background-color: #e0e0e0;
}

.nested-level {
  margin-left: 30rpx;
}

.subtask-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}

.subtask-progress {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #999;
  margin-top: 6rpx;
}

.progress-bar {
  flex: 1;
  height: 10rpx;
  background-color: #f0f0f0;
  border-radius: 5rpx;
  margin: 0 10rpx;
  overflow: hidden;
}

.progress-bar.nested {
  width: 100rpx;
}

.progress-fill {
  height: 100%;
  background-color: #0052d9;
  border-radius: 5rpx;
}

.progress-percentage {
  min-width: 40rpx;
  text-align: right;
}

/* 总体完成率样式 */
.overall-progress-section {
  background-color: white;
  border-radius: 16rpx;
  padding: 30rpx;
  margin-bottom: 30rpx;
}

.progress-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16rpx;
}

.progress-title {
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
}

.progress-percentage {
  font-size: 30rpx;
  color: #0052d9;
  font-weight: bold;
}

.progress-bar-container {
  width: 100%;
  height: 16rpx;
  background-color: #f5f5f5;
  border-radius: 8rpx;
  overflow: hidden;
}

.progress-bar-bg {
  width: 100%;
  height: 100%;
  position: relative;
}

.progress-bar-fill {
  height: 100%;
  background-color: #0052d9;
  border-radius: 8rpx;
  transition: width 0.3s ease;
}

/* 操作菜单样式 */
.action-menu-popup {
  position: absolute;
  top: 80rpx;
  right: 30rpx;
  background-color: white;
  border-radius: 12rpx;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.1);
  z-index: 100;
  overflow: hidden;
}

.menu-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 24rpx 30rpx;
  font-size: 28rpx;
  color: #333;
  border-bottom: 1rpx solid #f5f5f5;
}

.menu-item:last-child {
  border-bottom: none;
}

.menu-item.delete {
  color: #e34d59;
}

.menu-icon {
  margin-left: 20rpx;
  font-size: 28rpx;
}

.dropdown-menu {
  padding: 10rpx;
}

